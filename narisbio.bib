@article{Ryan2011,
	__Markedentry = {[hoohnaris:]},
	Abstract = {This article reports on a small group experiment studying how the preferences of an individual's social network affect her ability to vote for the candidate who will provide her with the greater benefit on both valence issues and position issues. The research diverges from traditional formal models and experimental studies of social communication by expanding the communication network beyond the dyad. The results suggest that social communication is a useful information shortcut for uninformed independents, but not uninformed partisans. Informed individuals incorporate biased social messages into their candidate evaluations, which results in higher levels of incorrect voting in certain types of networks.},
