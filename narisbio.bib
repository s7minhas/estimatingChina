@article{Dickson1992,
	__Markedentry = {[hoohnaris:]},
	Abstract = {Is political behavior in China a response to institutional or cultural signals? Recent publications take contrasting views in this enduring debate. Private citizens and public officials engage in clientelistic behavior appropriate to the hierarchical structure of the Communist political system, yet patterns of behavior in China have remained remarkably constant despite the institutional changes brought on first by the Communist revolution in 1949 and later by the reforms of the 1980s. This suggests that scholars ought to integrate the concepts of structure and culture by showing how traditional cultural values survive despite the efforts of political institutions to change them and how the political structure itself is shaped by the traditional culture.},
