@article{Casper2003,
	Abstract = {This article shows that highly correlated measures can produce different results. We identify a democratization model from the literature and test it in more than 120 countries from 1951 to 1992. Then, we check whether the results are robust regarding measures of democracy, time periods, and levels of development. The findings show that measures do matter: Whereas some of the findings are robust, most of them are not. This explains, in part, why the debates on democracy have continued rather than been resolved. More important, it underscores the need for more careful use of measures and further testing to increase confidence in the findings. Scholars in comparative politics are drawn increasingly to largeâ€N statistical analyses, often using data sets collected by others. As in any field, we show how they must be careful in choosing the most appropriate measures for their studies, without assuming that any correlated measure will do.},
	Author = {Casper, Gretchen and Tufis, Claudiu},
	Journal = {Political Analysis},
	Number = {2},
	Pages = {196-203},
	Title = {Correlation Versus Interchangeability: The Limited Robustness of Empirical Findings on Democracy Using Highly Correlated Data Sets},
	Type = {Journal Article},
	Volume = {11},
	Year = {2003}}

@article{Cassel1999,
	__Markedentry = {[hoohnaris:]},
	Abstract = {The article presents a review of the uneven history of research on nonreligious and religious group involvement in voting participation in the United States. In the 1970s and 1980s, a mass of new research on electoral turnout essentially disregarded the importance of participating in groups. Most students of political behavior apparently believed that voting--unlike more demanding forms of participation--does not require social contacts or information accessed through groups. Only recently have researchers returned to examining how religious and other voluntary groups spur turnout. This study looks at how voter turnout in U.S. presidential elections has been influenced by voluntary association-namely, activity in nonpolitical voluntary groups-and by church involvement. Three new conclusions emerge. First, relationships between nonreligious and religious group involvement and turnout are rather stable over time. Second, contrary to prevailing theories, for the most part neither voluntary association nor church involvement causes turnout by increasing predispositions to vote. Third, for the most part, neither nonreligious nor religious organizations leverage turnout through the influence of political leaders and issues.},
	Author = {Cassel, Carol A.},
	Journal = {Social Science Quarterly},
	Number = {3},
