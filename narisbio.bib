@article{Cox2004,
	__Markedentry = {[hoohnaris:]},
	Abstract = {This paper uses a three-games (or triadic) design to identify trusting and reciprocating behavior. A large literature on single-game trust and reciprocity experiments is based on the implicit assumption that subjects do not have altruistic or inequality-averse other-regarding preferences. Such experimental designs test compound hypotheses that include the hypothesis that other-regarding preferences do not affect behavior. In contrast, experiments with the triadic design do discriminate between transfers resulting from trust or reciprocity and transfers resulting from other-regarding preferences that are not conditional on the behavior of others. Decomposing trust from altruism and reciprocity from altruism or inequality aversion is critical to obtaining empirical information that can guide the process of constructing models that can increase the empirical validity of game theory.},
	Author = {Cox, James C.},
	Journal = {Games and Economic Behavior},
	Keywords = {Experimental economics Game theory Trust Reciprocity Altruism},
	Number = {2},
	Owner = {hoohnaris},
	Pages = {260-281},
	Timestamp = {2012.08.21},
	Title = {How to Identify Trust and Reciprocity},
	Volume = {46},
	Year = {2004}}

@book{Cox2009,
	__Markedentry = {[hoohnaris:]},
	Address = {Abingdon, UK},
	Author = {Cox, Michaelene},
	Keywords = {[]},
	Note = {This timely and very interesting volume is an important addition to the literature exploring the role that social capital can play in generating both conflict and peace building. Through theoretical interrogation of the relationship between social capital and conflict transformation, and discussion of a range of important grass-roots case studies, the volume explores theefficacy of social capital as a tool of both violence and peace. It also raises important questions about how social capital may become a more effective instrument in peace/conflict studies. It will arguably be of interest to a broad range of audiences in fields such as peace and conflict studies, international relations and sociology. The first section focuses on case studies that demonstrate the `darker' side of social capital -- where networks of trust, engagement and other aspects of social capital actually increase the ability of some groups to initiate and propagate violence. Some of the case studies explored in relation to this include groups in Northern Ireland, Columbia and Honduras, Eastern Europe and Russia. The second section then explores case studies where social capital has played an important role in understanding and addressing the origins of conflict, such as with groups in Sri Lanka, the former Yugoslavia, Mali and Cyprus. Building on these case studies, the final section addresses broader theoretical discussions about the opportunities, obstacles and efficacy of social capital as a tool for peace building. Overall, the book successfully brings together a range of theoretical perspectives and empirical cases. The section exploring theoretical issues and contexts is particularly useful for those who wish to understand the shifting role and importance of social capital within conflict resolution and peace-building discourses. The approach and focus of the case study chapters is quite varied, enabling a good overview of the various grassroots applications of social capital in the peace/conflict context -- including chapters on the utility of social capital for organised crime in Russia, how social capital has been used by the FARC in Colombia, social capital-based initiatives to prevent violence in Colombia and Honduras, and the rebuilding of inter-ethnic trust by anti-war women's groups in the former Yugoslavia. Overall, this volume is noteworthy for its discussions of the efficacy of social capital for conflict transformation, and the practical examples of where social capital has assisted grass-roots conflict resolution work. Victoria Mason (Lancaster University)},
	Owner = {hoohnaris},
	Publisher = {Routledge},
	Timestamp = {2012.08.21},
	Title = {Social Capital and Peace-Building: Creating and Resolving Conflict with Trust and Social Networks},
	Year = {2009}}

@article{Craig1990,
	__Markedentry = {[hoohnaris:]},
	Abstract = {Political efficacy and trust-among the most frequently used survey measures of general political attitudes-are often maligned for their lack of reliability and validity. This paper reports results from the National Election Studies 1987 pilot study, which included more than thirty-five efficacy and trust items. Five attitudinal dimensions were hypothesized; four emerged clearly. One scale, internal efficacy, is especially robust; a four- to six-item scale represents a considerable improvement on existing NES measures. External efficacy is distinguished from political trust, at least when the former is measured in terms of the fairness of political procedures and outcomes rather than in terms of elite responsiveness to popular demands. Though less decisive, there also is support for dividing trust into incumbent- and regime-based components. The failure to find a similar incumbent- and regime-based distinction for external efficacy is in accord with theoretical perspectives.},
	Author = {Craig, Stephen C. and Niemi, Richard G. and Silver, Glenn E.},
