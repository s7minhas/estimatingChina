@article{Miller1994,
	__Markedentry = {[hoohnaris:]},
	Abstract = {Using survey data collected in Russia, Ukraine, and Lithuanian in 1990-92, we reexamine findings reported by Finifter and Mickiewicz (1992). Our analysis indicates a significant link between political and economic reform orientations. Individuals who prefer political reforms of a democratic nature also favor a decreased role of the state in guaranteeing social well-being. In addition, our finding that better-educated Soviet and post-Soviet citizens are more likely than the less-well-educated to prefer individual responsibility for well-being is in direct contradiction to the findings reported by Finifter and Mickiewicz. The differences in the two sets of conclusions give rise to very different substantive conclusions regarding the economic and political changes now occurring in these societies.},
